{
  "name": "@transferlens/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript types and API client for TransferLens",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/client.ts src/types.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/client.ts src/types.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {},
  "keywords": [
    "transferlens",
    "api-client",
    "football",
    "transfers"
  ],
  "license": "MIT"
}
